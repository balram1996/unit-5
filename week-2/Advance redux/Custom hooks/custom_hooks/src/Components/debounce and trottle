function debouncer( delay, callback ){
  var debounce;
  return function(e){
      debounce && clearTimeout(debounce);
      debounce = setTimeout(function(){
          callback(e)
      },delay)
  }
}


function throttle( fn, wait ){
    let lastCall = 0;
    return function(){
    if( Date.now() - lastCall > wait  ){
      lastCall = Date.now()
      fn()
    }
  }
}
